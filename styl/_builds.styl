.jenkins
  &_build
    &_line
      margin : 0.5em
      background-color : #808e9b
      padding-left  : 0.5em
      padding-right : 0.5em
      border-radius : 0.2em

    &_status
      width : 3em
      height : 1em
      margin : 0.5em
      border-radius : 0.4em
      position : relative
      float : left
      text-align : center

    &_name
      float : left
      position : relative